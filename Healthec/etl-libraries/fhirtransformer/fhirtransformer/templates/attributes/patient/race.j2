{
  "url": "http://hl7.org/fhir/us/core/StructureDefinition/us-core-race",
  "extension": [
    {% if data.race_code %}
    {
      "url": "ombCategory",
      "valueCoding": {
        "system": "urn:oid:2.16.840.1.113883.6.238",
        "code": "{{ data.race_code }}",
        "display": "{{ data.race_display }}"
      }
    }
    {%- endif %}
    {%- if data.race and data.race_code -%}
    ,
    {%- endif %}
    {% if data.race %}
    {
      "url": "text",
      "valueString": "{{ data.race }}"
    }
    {%- endif %}
  ]
}