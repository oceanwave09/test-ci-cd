{
  "url": "http://hl7.org/fhir/us/core/StructureDefinition/us-core-ethnicity",
  "extension": [
    {% if data.ethnicity_code %}
    {
      "url": "ombCategory",
      "valueCoding": {
        "system": "urn:oid:2.16.840.1.113883.6.238",
        "code": "{{ data.ethnicity_code }}",
        "display": "{{ data.ethnicity_display }}"
      }
    }
    {%- endif %}
    {%- if data.ethnicity and data.ethnicity_code -%}
    ,
    {%- endif %}
    {% if data.ethnicity %}
    {
      "url": "text",
      "valueString": "{{ data.ethnicity }}"
    }
    {%- endif %}
  ]
}