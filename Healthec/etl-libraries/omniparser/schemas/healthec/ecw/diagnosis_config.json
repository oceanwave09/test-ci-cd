{
  "parser_settings": {
    "version": "omni.2.1",
    "file_format_type": "csv2"
  },
  "file_declaration": {
    "delimiter": "|",
    "replace_double_quotes": true,
    "records": [
      {
        "rows": 1,
        "min": 1,
        "max": 1
      },
      {
        "is_target": true,
        "columns": [
          {
            "name": "apuid",
            "index": 1
          },
          {
            "name": "record_id",
            "index": 2
          },
          {
            "name": "patient_id",
            "index": 3
          },
          {
            "name": "patient_account_no",
            "index": 4
          },
          {
            "name": "encounter_id",
            "index": 5
          },
          {
            "name": "encounter_date",
            "index": 6
          },
          {
            "name": "provider_npi",
            "index": 7
          },
          {
            "name": "icd_code",
            "index": 8
          },
          {
            "name": "name",
            "index": 9
          },
          {
            "name": "assessment_onset_date",
            "index": 10
          }
        ]
      }
    ]
  },
  "transform_declarations": {
    "FINAL_OUTPUT": {
      "object": {
        "row_id": {
          "custom_func": {
            "name": "uuidv4"
          }
        },
        "apuid": {
          "xpath": "apuid"
        },
        "record_id": {
          "xpath": "record_id"
        },
        "patient_id": {
          "xpath": "patient_id"
        },
        "patient_account_no": {
          "xpath": "patient_account_no"
        },
        "encounter_id": {
          "xpath": "encounter_id"
        },
        "encounter_date": {
          "xpath": "encounter_date"
        },
        "provider_npi": {
          "custom_func": {
            "name": "encryptAesEcb",
            "args": [
              {
                "external": "data_key"
              },
              {
                "xpath": "provider_npi"
              }
            ]
          }
        },
        "icd_code": {
          "xpath": "icd_code"
        },
        "name": {
          "xpath": "name"
        },
        "assessment_onset_date": {
          "xpath": "assessment_onset_date"
        }
      }
    }
  }
}