{
  "parser_settings": {
    "version": "omni.2.1",
    "file_format_type": "fixedlength2"
  },
  "file_declaration": {
    "envelopes": [
      {
        "columns": [
          { "name": "CUR_CLM_UNIQ_ID", "start_pos": 1, "length": 13 },
          { "name": "CLM_LINE_NUM", "start_pos": 14, "length": 10 },
          { "name": "BENE_MBI_ID", "start_pos": 24, "length": 11 },
          { "name": "BENE_HIC_NUM", "start_pos": 35, "length": 11 },
          { "name": "CLM_TYPE_CD", "start_pos": 46, "length": 2 },
          { "name": "CLM_FROM_DT", "start_pos": 48, "length": 10 },
          { "name": "CLM_THRU_DT", "start_pos": 58, "length": 10 },
          { "name": "CLM_FED_TYPE_SRVC_CD", "start_pos": 68, "length": 1 },
          { "name": "CLM_POS_CD", "start_pos": 69, "length": 2 },
          { "name": "CLM_LINE_FROM_DT", "start_pos": 71, "length": 10 },
          { "name": "CLM_LINE_THRU_DT", "start_pos": 81, "length": 10 },
          { "name": "CLM_LINE_HCPCS_CD", "start_pos": 91, "length": 5 },
          { "name": "CLM_LINE_CVRD_PD_AMT", "start_pos": 96, "length": 15 },
          { "name": "CLM_PRMRY_PYR_CD", "start_pos": 111, "length": 1 },
          { "name": "PAYTO_PRVDR_NPI_NUM", "start_pos": 112, "length": 10 },
          { "name": "ORDRG_PRVDR_NPI_NUM", "start_pos": 122, "length": 10 },
          { "name": "CLM_CARR_PMT_DNL_CD", "start_pos": 132, "length": 2 },
          { "name": "CLM_PRCSG_IND_CD", "start_pos": 134, "length": 2 },
          { "name": "CLM_ADJSMT_TYPE_CD", "start_pos": 136, "length": 2 },
          { "name": "CLM_EFCTV_DT", "start_pos": 138, "length": 10 },
          { "name": "CLM_IDR_LD_DT", "start_pos": 148, "length": 10 },
          { "name": "CLM_CNTL_NUM", "start_pos": 158, "length": 40 },
          { "name": "BENE_EQTBL_BIC_HICN_NUM", "start_pos": 198, "length": 11 },
          { "name": "CLM_LINE_ALOWD_CHRG_AMT", "start_pos": 209, "length": 17 },
          { "name": "CLM_DISP_CD", "start_pos": 226, "length": 2 },
          { "name": "CLM_BLG_PRVDR_NPI_NUM", "start_pos": 228, "length": 10 },
          { "name": "CLM_RFRG_PRVDR_NPI_NUM", "start_pos": 238, "length": 10 }
        ]
      }
    ]
  },
  "transform_declarations": {
    "FINAL_OUTPUT": {
      "object": {
        "row_id": { "template": "uuidv4" },
        "cur_clm_uniq_id": { "xpath": "CUR_CLM_UNIQ_ID", "type": "int" },
        "clm_line_num": { "xpath": "CLM_LINE_NUM", "type": "int" },
        "bene_mbi_id": {
          "custom_func": {
            "name": "encryptAesEcb",
            "args": [
              {
                "external": "data_key"
              },
              {
                "xpath": "BENE_MBI_ID",
                "template": "remove_special_char"
              }
            ]
          }
        },
        "bene_hic_num": {
          "custom_func": {
            "name": "encryptAesEcb",
            "args": [
              {
                "external": "data_key"
              },
              {
                "xpath": "BENE_HIC_NUM",
                "template": "remove_special_char"
              }
            ]
          }
        },
        "clm_type_cd": { "xpath": "CLM_TYPE_CD", "type": "int" },
        "clm_from_dt": {
          "xpath": "CLM_FROM_DT",
          "template": "remove_special_char"
        },
        "clm_thru_dt": {
          "xpath": "CLM_THRU_DT",
          "template": "remove_special_char"
        },
        "clm_fed_type_srvc_cd": {
          "xpath": "CLM_FED_TYPE_SRVC_CD",
          "template": "remove_special_char"
        },
        "clm_pos_cd": {
          "xpath": "CLM_POS_CD",
          "template": "remove_special_char"
        },
        "clm_line_from_dt": {
          "xpath": "CLM_LINE_FROM_DT",
          "template": "remove_special_char"
        },
        "clm_line_thru_dt": {
          "xpath": "CLM_LINE_THRU_DT",
          "template": "remove_special_char"
        },
        "clm_line_hcpcs_cd": {
          "xpath": "CLM_LINE_HCPCS_CD",
          "template": "remove_special_char"
        },
        "clm_line_cvrd_pd_amt": {
          "xpath": "CLM_LINE_CVRD_PD_AMT",
          "type": "float"
        },
        "clm_prmry_pyr_cd": {
          "xpath": "CLM_PRMRY_PYR_CD",
          "template": "remove_special_char"
        },
        "payto_prvdr_npi_num": {
          "xpath": "PAYTO_PRVDR_NPI_NUM",
          "template": "remove_special_char"
        },
        "ordrg_prvdr_npi_num": {
          "xpath": "ORDRG_PRVDR_NPI_NUM",
          "template": "remove_special_char"
        },
        "clm_carr_pmt_dnl_cd": {
          "xpath": "CLM_CARR_PMT_DNL_CD",
          "template": "remove_special_char"
        },
        "clm_prcsg_ind_cd": {
          "xpath": "CLM_PRCSG_IND_CD",
          "template": "remove_special_char"
        },
        "clm_adjsmt_type_cd": {
          "xpath": "CLM_ADJSMT_TYPE_CD",
          "template": "remove_special_char"
        },
        "clm_efctv_dt": {
          "xpath": "CLM_EFCTV_DT",
          "template": "remove_special_char"
        },
        "clm_idr_ld_dt": {
          "xpath": "CLM_IDR_LD_DT",
          "template": "remove_special_char"
        },
        "clm_cntl_num": {
          "xpath": "CLM_CNTL_NUM",
          "template": "remove_special_char"
        },
        "bene_eqtbl_bic_hicn_num": {
          "xpath": "BENE_EQTBL_BIC_HICN_NUM",
          "template": "remove_special_char"
        },
        "clm_line_alowd_chrg_amt": {
          "xpath": "CLM_LINE_ALOWD_CHRG_AMT",
          "type": "float"
        },
        "clm_disp_cd": {
          "xpath": "CLM_DISP_CD",
          "template": "remove_special_char"
        },
        "clm_blg_prvdr_npi_num": {
          "xpath": "CLM_BLG_PRVDR_NPI_NUM",
          "template": "remove_special_char"
        },
        "clm_rfrg_prvdr_npi_num": {
          "xpath": "CLM_RFRG_PRVDR_NPI_NUM",
          "template": "remove_special_char"
        }
      }
    },
    "uuidv4": {
      "custom_func": {
        "name": "javascript_with_context",
        "args": [
          {
            "const": "var date_time = new Date().getTime(); var row_uuid = 'xxxxxxxx-xxxx-4xxx-yxxx-xxxxxxxxxxxx'.replace(/[xy]/g, function(param) {  var generate_id = (date_time + Math.random()*16)%16 | 0; date_time = Math.floor(date_time/16); return (param =='x' ? generate_id : (generate_id&0x3|0x8)).toString(16);}); row_uuid"
          }
        ],
        "ignore_error": true
      }
    },
    "remove_special_char": {
      "custom_func": {
        "name": "javascript_with_context",
        "args": [
          {
            "const": "var get_value = JSON.parse(_node); get_value.replace(/^~|~$/g, '')"
          }
        ],
        "ignore_error": false
      }
    }
  }
}
