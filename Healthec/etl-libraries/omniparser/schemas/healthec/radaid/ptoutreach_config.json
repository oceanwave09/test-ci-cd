{
    "parser_settings": {
      "version": "omni.2.1",
      "file_format_type": "csv2"
    },
    "file_declaration": {
      "delimiter": ",",
      "replace_double_quotes": false,
      "records": [
        {
          "rows": 1,
          "min": 1,
          "max": 1
        },
        {
          "is_target": true,
          "columns": [
            {
              "name": "batch_type",
              "index": 1
            },
            {
              "name": "first_name",
              "index": 2
            },
            {
              "name": "last_name",
              "index": 3
            },
            {
              "name": "mrn",
              "index": 4
            },
            {
              "name": "outreach_type",
              "index": 5
            },
            {
              "name": "pn_contact",
              "index": 6
            },
            {
              "name": "pn_contact_date",
              "index": 7
            },
            {
              "name": "pn_barrier",
              "index": 8
            },
            {
              "name": "outcome",
              "index": 9
            }
          ]
        }
      ]
    },
    "transform_declarations": {
      "FINAL_OUTPUT": {
        "object": {
          "row_id": {
            "custom_func": {
              "name": "uuidv4"
            }
          },
          "batch_type": {
            "xpath": "batch_type"
          },
          "first_name": {
            "custom_func": {
              "name": "encryptAesEcb",
              "args": [
                {
                  "external": "data_key"
                },
                {
                  "xpath": "first_name"
                }
              ]
            }
          },
          "last_name": {
            "custom_func": {
              "name": "encryptAesEcb",
              "args": [
                {
                  "external": "data_key"
                },
                {
                  "xpath": "last_name"
                }
              ]
            }
          },
          "mrn": {
            "custom_func": {
              "name": "encryptAesEcb",
              "args": [
                {
                  "external": "data_key"
                },
                {
                  "xpath": "mrn"
                }
              ]
            }
          },
          "outreach_type": {
            "xpath": "outreach_type"
          },
          "pn_contact": {
            "xpath": "pn_contact"
          },
          "pn_contact_date": {
            "xpath": "pn_contact_date"
          },
          "pn_barrier": {
            "xpath": "pn_barrier"
          },
          "outcome": {
            "xpath": "outcome"
          }
        }
      }
    }
  }